<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.2"
    xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file original="" product-name="Litefyr.Style" source-language="en" target-language="de" datatype="plaintext">
        <body>
            <trans-unit id="groups.themeColorScheme" xml:space="preserve">
                <source>Color scheme</source>
                <target>Farbschema</target>
            </trans-unit>
            <trans-unit id="groups.themeColorsLight" xml:space="preserve">
                <source>Light Mode</source>
                <target>Helle Darstellung</target>
            </trans-unit>
            <trans-unit id="groups.themeColorsDark" xml:space="preserve">
                <source>Dark Mode</source>
                <target>Dunkle Darstellung</target>
            </trans-unit>
            <trans-unit id="groups.themeColorContrast" xml:space="preserve">
                <source>Color contrast</source>
                <target>Farbkontrast</target>
            </trans-unit>
            <trans-unit id="properties.themeColorScheme.selectBoxEditor.values.light dark" xml:space="preserve">
                <source>Light and dark mode</source>
                <target>Helle und dunkle Darstellung</target>
            </trans-unit>
            <trans-unit id="properties.themeColorSchemeSwitchter" xml:space="preserve">
                <source>Show selection for dark mode</source>
                <target>Auswahl für Dark Mode anzeigen</target>
            </trans-unit>
            <trans-unit id="properties.themeColorSchemeSwitchter.selectBoxEditor.placeholder" xml:space="preserve">
                <source>Do not display</source>
                <target>Nicht anzeigen</target>
            </trans-unit>
            <trans-unit id="properties.themeColorSchemeSwitchter.selectBoxEditor.values.top" xml:space="preserve">
                <source>In the navigation bar</source>
                <target>In der Navigationsleiste</target>
            </trans-unit>
            <trans-unit id="properties.themeColorSchemeSwitchter.selectBoxEditor.values.bottom" xml:space="preserve">
                <source>In the footer</source>
                <target>In der Fußzeile</target>
            </trans-unit>
            <trans-unit id="properties.themeColorSchemeSwitchter.selectBoxEditor.values.both" xml:space="preserve">
                <source><![CDATA[Top & bottom]]></source>
                <target><![CDATA[Oben & unten]]></target>
            </trans-unit>
            <trans-unit id="backgroundAndText" xml:space="preserve">
                <source>Background and text color of the website</source>
                <target>Hintergrund- und Textfarbe der Website</target>
            </trans-unit>
            <trans-unit id="header" xml:space="preserve">
                <source>Header color</source>
                <target>Farbe der Kopfzeile</target>
            </trans-unit>
            <trans-unit id="footer" xml:space="preserve">
                <source>Footer color</source>
                <target>Farbe der Fußzeile</target>
            </trans-unit>
            <trans-unit id="main" xml:space="preserve">
                <source>Main accent color</source>
                <target>Hauptakzentfarbe</target>
            </trans-unit>
            <trans-unit id="minor" xml:space="preserve">
                <source>Minor accent color</source>
                <target>Zweite Akzentfarbe</target>
            </trans-unit>
            <trans-unit id="minor2" xml:space="preserve">
                <source>Third accent color</source>
                <target>Dritte Akzentfarbe</target>
            </trans-unit>
            <trans-unit id="properties.themeColorContrastThreshold" xml:space="preserve">
                <source>Threshold for contrast color</source>
                <target>Schwellenwert für Kontrastfarbe</target>
            </trans-unit>
            <trans-unit id="properties.themeColorContrastThreshold.ui.help.message" xml:space="preserve">
                <source>Changes the threshold when the contrast color switches to black or white. The higher the number, the more likely white will be chosen as the contrast color.</source>
                 <target>Ändert den Schwellenwert, wenn die Kontrastfarbe zu Schwarz oder Weiß wechselt. Je höher der Wert, desto eher wird Weiß als Kontrastfarbe gewählt.</target>
            </trans-unit>
            <trans-unit id="properties.themeColorContrastLightnessMin" xml:space="preserve">
                <source>Minimum contrast lightness</source>
                <target>Minimale Kontrast Helligkeit</target>
            </trans-unit>
            <trans-unit id="properties.themeColorContrastLightnessMin.ui.help.message" xml:space="preserve">
                <source>A value higher than 0 adds a hint of hue to the black.</source>
                <target>Ein Wert höher als 0 fügt dem Schwarz einen leichte Tönung hinzu.</target>
            </trans-unit>
            <trans-unit id="properties.themeColorContrastLightnessMax" xml:space="preserve">
                <source>Maximum contrast lightness</source>
                <target>Maximale Kontrast Helligkeit</target>
            </trans-unit>
            <trans-unit id="properties.themeColorContrastLightnessMax.ui.help.message" xml:space="preserve">
                <source>A value lower than 100 adds a hint of hue to the white.</source>
                <target>Ein Wert tiefer als 100 fügt dem Weiß einen leichte Tönung hinzu.</target>
            </trans-unit>
        </body>
    </file>
</xliff>
