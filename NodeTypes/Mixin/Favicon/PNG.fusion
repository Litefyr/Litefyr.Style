favicon-png = Litefyr.Style:Favicon.Case {
    contentType = 'image/png'
    content = Litefyr.Style:Favicon.Content {
        type = 'png'
        asset = ${q(site).property('themeFaviconPng')}
        size = 180
        node = ${site}
    }
}

